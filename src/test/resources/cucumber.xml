<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <import resource="application-context-test.xml" />

    <bean id="nonLockingDequeueStrategy" class="com.github.rtmax0.redisq.queuing.FIFOQueueDequeueStrategy">
        <property name="dequeueTimeoutSeconds" value="1"/>
    </bean>
    <bean id="stringSerializer" class="com.github.rtmax0.redisq.serialization.StringPayloadSerializer"/>
</beans>
